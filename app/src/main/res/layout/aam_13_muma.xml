<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_Layout"
    tools:context=".pantallas.muma.pMUMA">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/include"
            layout="@layout/main_toolbahorizontal"
            />

        <RelativeLayout
            android:id="@+id/rllbo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <WebView
                android:id="@+id/wbvMuma"
                android:layout_width="match_parent"
                android:layout_height="match_parent"></WebView>


                        <ImageView
                            android:id="@+id/fabmumaopen"
                            android:layout_width="70dp"
                            android:layout_height="70dp"
                            android:layout_gravity="left|bottom"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginBottom="3dp"
                            android:background="@drawable/circlebackground"
                            android:backgroundTint="@color/azulitofuerte"
                            android:src="@drawable/logofamexblanco2023" />

                        <ImageView
                            android:id="@+id/fabmumaclose"
                            android:layout_width="70dp"
                            android:layout_height="70dp"
                            android:layout_gravity="left|bottom"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginBottom="3dp"
                            android:background="@drawable/circlebackground"
                            android:backgroundTint="@color/azulitofuerte"
                            android:src="@drawable/logofamexblanco2023"
                            android:visibility="gone" />

                        <FrameLayout
                            android:id="@+id/pppmuma"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginTop="79dp"
                            android:background="@color/muma2"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="90dp"
                                android:layout_marginRight="90dp"
                                android:orientation="vertical"
                                android:weightSum="11">

                                <LinearLayout
                                    android:id="@+id/linearLayout7"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="10"
                                    android:background="@color/muma1"
                                    android:gravity="center"
                                    android:orientation="vertical"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    tools:ignore="MissingConstraints">

                                    <androidx.viewpager.widget.ViewPager
                                        android:id="@+id/vprpop1"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="top"
                                        android:background="@color/transparencia"></androidx.viewpager.widget.ViewPager>
                                </LinearLayout>


                                <LinearLayout
                                    android:id="@+id/llymuma"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@color/azulitofuerte"
                                    android:gravity="top|center"
                                    android:orientation="horizontal"></LinearLayout>
                            </LinearLayout>

                        </FrameLayout>
                        </RelativeLayout>

                </LinearLayout>


            </androidx.drawerlayout.widget.DrawerLayout>